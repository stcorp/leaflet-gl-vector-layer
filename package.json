{
  "name": "@stcorp/leaflet-gl-vector-layer",
  "version": "0.1.0",
  "description": "A GL-powered vector layer for leaflet",
  "main": "./dist/leaflet-gl-vector-layer.umd.js",
  "module": "./dist/leaflet-gl-vector-layer.es.js",
  "types": "./dist/typings.d.ts",
  "author": "Martti Kaipiainen",
  "license": "0BSD",
  "exports": {
    ".": {
      "import": "./dist/leaflet-gl-vector-layer.es.js",
      "require": "./dist/leaflet-gl-vector-layer.umd.js"
    }
  },
  "files": [
    "dist/**"
  ],
  "scripts": {
    "start": "vite dev",
    "build": "rm -rf ./dist && vite build --config vite.build.config.ts && tsc --emitDeclarationOnly --declaration --outDir ./dist/types && rollup --config rollup.types.config.js && rm -rf ./dist/types"
  },
  "devDependencies": {
    "@types/chroma-js": "^2.1.3",
    "@types/leaflet": "^1.7.9",
    "esbuild": "^0.14.18",
    "eslint": "^8.8.0",
    "leaflet": "^1.7.1",
    "rollup": "^2.67.0",
    "rollup-plugin-dts": "^4.1.0",
    "rollup-plugin-esbuild": "^4.8.2",
    "sass": "^1.49.8",
    "typescript": "^4.5.5",
    "vite": "^2.8.4",
    "vite-plugin-dts": "^0.9.9",
    "vite-plugin-static-copy": "^0.2.0"
  },
  "dependencies": {
    "@jaames/iro": "^5.5.2",
    "chroma-js": "^2.4.2",
    "rxjs": "^7.5.4"
  },
  "peerDependencies": {
    "leaflet": "^1.7.1"
  }
}
